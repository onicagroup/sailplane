{
  "name": "@sailplane/injector",
  "version": "4.0.0-alpha.2",
  "description": "Simple, light-weight, lazy-instantiating, and type-safe dependency injection in Typescript.",
  "keywords": [
    "di",
    "dependency injection",
    "typescript"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig-cjs.json && ../publish-modules.sh dist && npm link && cp dist/esm/injector.d.ts ../docs/types/",
    "test": "LOG_LEVEL=NONE jest --no-cache",
    "test:watch": "jest --watch",
    "clean:publish": "rm -r dist; npm run build && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onicagroup/sailplane.git"
  },
  "license": "Apache-2.0",
  "author": "Onica Group",
  "homepage": "https://docs.onica.com/projects/sailplane",
  "contributors": [
    "Adam Fanello <adam.fanello@rackspace.com>"
  ],
  "dependencies": {
    "reflect-metadata": "~0.1.13"
  },
  "devDependencies": {
    "@sailplane/logger": "file:../logger",
    "@types/jest": "^29.2.3",
    "@types/node": "^14.18.33",
    "bottlejs": "^2.0.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@sailplane/logger": ">=2.0.0",
    "bottlejs": "2.x.x"
  },
  "main": "dist/cjs/injector.js",
  "module": "dist/esm/injector.js",
  "exports": {
    ".": {
      "import": "./dist/esm/injector.js",
      "require": "./dist/cjs/injector.js"
    }
  },
  "files": [
    "dist/cjs/injector.js",
    "dist/cjs/injector.js.map",
    "dist/cjs/injector.d.ts",
    "dist/cjs/package.json",
    "dist/esm/injector.js",
    "dist/esm/injector.js.map",
    "dist/esm/injector.d.ts",
    "dist/esm/package.json"
  ]
}
